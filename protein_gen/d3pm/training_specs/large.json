{
	"training_output_dir": "training_output",
	"model_version": "large",
	"data_mod_kwargs": {
		"data_dir": "../../data/uniref50_small",
		"train_data_fname": "train_data.csv",
		"test_data_fname": "test_data.csv",
		"val_data_fname": "val_data.csv",
		"batch_size": 128,
		"num_workers": 4,
		"taxon_dropout_rate": 0.2,
		"attribute_dropout_rate": 0.2
	},
	"model_config_kwargs": {
		"d_model": 128,
		"mask_p_floor": 0.01,
		"transformer_kwargs": {
			"nhead": 8,
			"num_encoder_layers": 1,
			"num_decoder_layers": 5,
			"dim_feedforward": 192,
			"dropout": 0.1,
			"activation": "gelu",
			"norm_first": true
		},
		"optimizer_kwargs": {
			"betas": [0.9, 0.95],
			"lr": 1e-5,
			"weight_decay": 1e-3
		},
		"reduce_lr_on_plateau_kwargs": {
			"patience": 7,
			"factor": 0.5,
			"cooldown": 1,
			"verbose": true
		}
	},
	"callbacks": {
		"LearningRateMonitor": {},
		"EarlyStopping": {
			"monitor": "val_loss",
			"patience": 25
		},
		"StochasticWeightAveraging": {
			"swa_lrs": 1e-3
		}
	},
	"trainer_kwargs": {
		"max_epochs": -1,
		"accumulate_grad_batches": 20,
		"gradient_clip_val": 0.5,
		"precision": 16
	}
}